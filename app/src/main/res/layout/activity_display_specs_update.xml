<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DisplaySpecsUpdateActivity">

    <TextView
        android:id="@+id/name"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="18dp"
        android:text="Name"
        app:layout_constraintBottom_toTopOf="@+id/processortxt"
        app:layout_constraintStart_toStartOf="@+id/processortxt"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/processortxt"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="12dp"
        android:text="Processor"
        app:layout_constraintBottom_toTopOf="@+id/ram"
        app:layout_constraintStart_toStartOf="@+id/ram"
        app:layout_constraintTop_toBottomOf="@+id/name" />

    <TextView
        android:id="@+id/ram"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="11dp"
        android:text="Memory"
        app:layout_constraintBottom_toTopOf="@+id/hdd"
        app:layout_constraintStart_toStartOf="@+id/hdd"
        app:layout_constraintTop_toBottomOf="@+id/processortxt" />

    <TextView
        android:id="@+id/hdd"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="74dp"
        android:text="HDD"
        app:layout_constraintBottom_toTopOf="@+id/brand"
        app:layout_constraintStart_toStartOf="@+id/vga"
        app:layout_constraintTop_toBottomOf="@+id/ram" />

    <TextView
        android:id="@+id/vga"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:text="VGA"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/brand"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.476" />

    <TextView
        android:id="@+id/brand"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="22dp"
        android:text="Brand"
        app:layout_constraintBottom_toTopOf="@+id/model"
        app:layout_constraintStart_toStartOf="@+id/model"
        app:layout_constraintTop_toBottomOf="@+id/hdd" />

    <TextView
        android:id="@+id/model"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="164dp"
        android:text="Model"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/screen"
        app:layout_constraintTop_toBottomOf="@+id/brand" />

    <TextView
        android:id="@+id/screen"
        android:layout_width="80dp"
        android:layout_height="45dp"
        android:layout_marginEnd="24dp"
        android:layout_marginBottom="92dp"
        android:text="Screen"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/Screen" />


    <EditText
        android:id="@+id/Name"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginTop="32dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintStart_toStartOf="@+id/Processorf"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Processorf"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginTop="95dp"
        android:layout_marginBottom="12dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Memory"
        app:layout_constraintStart_toStartOf="@+id/Memory"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/Memory"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginBottom="11dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Hdd"
        app:layout_constraintStart_toStartOf="@+id/Hdd"
        app:layout_constraintTop_toBottomOf="@+id/Processorf" />

    <EditText
        android:id="@+id/Screen"
        android:layout_width="250dp"
        android:layout_height="45dp"
        android:layout_marginEnd="36dp"
        android:layout_marginBottom="28dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Save"
        app:layout_constraintEnd_toEndOf="@+id/barrier3" />

    <EditText
        android:id="@+id/Hdd"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginBottom="13dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Vga"
        app:layout_constraintStart_toStartOf="@+id/Vga"
        app:layout_constraintTop_toBottomOf="@+id/Memory" />

    <EditText
        android:id="@+id/Vga"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginBottom="16dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Brand"
        app:layout_constraintStart_toStartOf="@+id/Brand"
        app:layout_constraintTop_toBottomOf="@+id/Hdd" />

    <EditText
        android:id="@+id/Brand"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginBottom="22dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Model"
        app:layout_constraintStart_toStartOf="@+id/Model"
        app:layout_constraintTop_toBottomOf="@+id/Vga" />

    <EditText
        android:id="@+id/Model"
        android:layout_width="250dp"
        android:layout_height="0dp"
        android:layout_marginBottom="107dp"
        android:ems="10"
        android:inputType="textPersonName"
        app:layout_constraintBottom_toTopOf="@+id/Save"
        app:layout_constraintStart_toStartOf="@+id/Screen"
        app:layout_constraintTop_toBottomOf="@+id/Brand" />

    <TextView
        android:id="@+id/tv_response"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/computer_primary_id"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:visibility="gone"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/Save"
        android:layout_width="80dp"
        android:layout_height="0dp"
        android:layout_marginBottom="17dp"
        android:text="Save"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/Screen" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group16"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="411dp" />

    <androidx.constraintlayout.widget.Group
        android:id="@+id/group17"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toTopOf="parent"
        app:layout_constraintEnd_toStartOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.constraintlayout.widget.Barrier
        android:id="@+id/barrier3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:barrierDirection="left"
        tools:layout_editor_absoluteX="411dp" />


</androidx.constraintlayout.widget.ConstraintLayout>